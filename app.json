{
  "name": "Give Up",
  "displayName": "Give Up",
  "expo": {
    "name": "Give Up",
    "slug": "give-up",
    "scheme": "giveup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../assets/images/Just Give Up Circle Logo.png",
    "splash": {
      "image": "../assets/images/Just Give Up Circle Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#191015"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "icon": ".../assets/images/Just Give Up Circle Logo.png",
      "package": "com.giveup",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-android-adaptive-foreground.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "../assets/images/Just Give Up Circle Logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "ios": {
      "icon": "../assets/images/Just Give Up Circle Logo.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.giveup",
      "splash": {
        "image": "../assets/images/Just Give Up Circle Logo.png",
        "tabletImage": "../assets/images/Just Give Up Circle Logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "web": {
      "favicon": "../assets/images/Just Give Up Circle Logo.png",
      "splash": {
        "image": "../assets/images/Just Give Up Circle Logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false,
            "flipper": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "62f082c1-1f41-4210-9b79-18bef36a6c5f"
      }
    }
  },
  "ignite": {
    "version": "9.6.2"
  }
}
